{
    "parserOptions": {
        "ecmaVersion": 2017,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "browser": true
    },
    "plugins": [
        "jsx-a11y"
    ],
    "extends": [
        "eslint:recommended", 
        "plugin:jsx-a11y/recommended",
        "plugin:compat/recommended"
    ],
    "rules": {
        "semi": 1,
        "no-console": 1,
        "no-cond-assign":1,
        "no-extra-boolean-cast":1,
        "no-unreachable":2,
        "no-inner-declarations":2,
        "consistent-return":2,
        "no-func-assign":2,
        "eqeqeq":[2, "smart"],
        "no-invalid-this":2,
        "no-global-assign": 2,
        "no-const-assign":2,
        "no-this-before-super":2,
        "no-else-return":2,
        "no-var":2,
        "compat/compat": 2,
        "jsx-a11y/alt-text": 2

    }
}